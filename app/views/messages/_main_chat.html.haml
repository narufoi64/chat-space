  .chat-main__message-form
    .new_message
      .input-box
        .input-box__text
          type a message
        %label.input-box__image{for: "message_image"}
          = icon('fa fa-image',  class: "fa-image")
          %input{type: "file", class: "input-box__image__file", name: "message[image]",id: "message_image"}
      .submit-btn