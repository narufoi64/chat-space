  .chat-main__message-list
    .chat-main__message-list_box
      .chat-main__message-list_box__upper-info
        %p.chat-main__message-list_box__upper-info__talk
          masa
        %p.chat-main__message-list_box__upper-info__date
          2019/12/6(Fri)21:54
      %p.chat-main__message-list__lower-info
        おはよう
    .chat-main__message-list_box
      .chat-main__message-list_box__upper-info
        %p.chat-main__message-list_box__upper-info__talk
          masa
        %p.chat-main__message-list_box__upper-info__date
          2019/12/6(Fri)21:54
      %p.chat-main__message-list__lower-info
